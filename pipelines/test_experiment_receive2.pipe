#
# Testing connections to serializer process
#
process zmq :: zmq_transport_receive
  port = 6560
  num_publishers = 1

# --------------------------------------------------
process dser :: deserializer
        serialization_type = protobuf 

connect from zmq.serialized_message to dser.dos

# --------------------------------------------------
process sink_dos :: detected_object_output
        file_name = received_dos.csv
        writer:type = csv

connect from dser.dos/detected_object_set to sink_dos.detected_object_set


# -------------------------------------------------
